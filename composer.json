{
    "name": "elstc/cakephp-restore-query",
    "description": "Retaining and Restoring query strings plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.0,<3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Elastic\\RestoreQuery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elastic\\RestoreQuery\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
